{
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:alanpoulain/core.git"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "api-platform/core": "dev-feature/graphql",
        "doctrine/annotations": "^1.0",
        "doctrine/data-fixtures": "^1.2",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.4.5",
        "guzzlehttp/guzzle": "^6.0",
        "hautelook/alice-bundle": "^2.0@beta",
        "nelmio/alice": "^3.0",
        "nelmio/cors-bundle": "^1.5",
        "phpdocumentor/reflection-docblock": "^3.0",
        "symfony/asset": "^3.0",
        "symfony/expression-language": "^3.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/security-bundle": "^3.0",
        "symfony/twig-bundle": "^3.0",
        "symfony/validator": "^3.0",
        "symfony/yaml": "^3.3",
        "theofidry/alice-data-fixtures": "^1.0@beta",
        "webonyx/graphql-php": "^0.11.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script",
            "assets:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BTJCC7GJH3JH3BA42W10TQ98",
            "allow-contrib": false
        }
    }
}
